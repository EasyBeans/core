<easybeans xmlns="http://org.ow2.easybeans.server">

    <!-- No infinite loop: wait="false"
         Enable MBeans: mbeans="true"
         Enable EasyBeans naming: naming="true"
         Use EasyBeans JACC provider: jacc="true"
         Disable EasyBeans file monitoring: scanning="false"
         Use EasyBeans JMX Connector: connector="true"
         Enable Deployer and J2EEServer MBeans: deployer="true" & j2eeserver="true"
    -->
    
    <config
       wait="false"
       mbeans="true"
       naming="true"
       jacc="true"
       scanning="false"
       connector="true"
       deployer="true"
       j2eeserver="true" />
       
</easybeans>
