<persistence xmlns="http://java.sun.com/xml/ns/persistence"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
         http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
     version="1.0">
<persistence-unit name="easybeans-default-persistence" transaction-type="JTA">

   <properties>
     <property name="org.eclipse.persistence.jpa.PersistenceProvider" value="org/ow2/easybeans/persistence/conf/eclipselink-persistence.xml"/>
     <property name="org.hibernate.ejb.HibernatePersistence" value="org/ow2/easybeans/persistence/conf/hibernate-persistence.xml"/>
     <property name="oracle.toplink.essentials.PersistenceProvider" value="org/ow2/easybeans/persistence/conf/toplink-persistence.xml"/>
     <property name="org.apache.openjpa.persistence.PersistenceProviderImpl" value="org/ow2/easybeans/persistence/conf/openjpa-persistence.xml"/>
   </properties>
</persistence-unit>
</persistence>
